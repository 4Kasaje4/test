const restrictionSegment: { segmentID: number; campaignID: string }[] = [];

// =========================== [END] Master Data ================================

enum ETargetTypeValue {
  COUNTRY = "COUNTRY",
  ZONE = "ZONE",
  PROVINCE = "PROVINCE",
  DISTRICT = "DISTRICT",
  SEGMENT = "SEGMENT",
  RETAILER = "RETAILER",
}

const getTargetTypeValue = (
  campaignID: string,
  targetType: ETargetTypeValue
) => {
  console.log(targetType, "targetType");
  switch (targetType) {
    case ETargetTypeValue.COUNTRY: {
      return [1];
      break;
    }
    case ETargetTypeValue.ZONE: {
      const filterResZone = restrictionZone.filter(
        (resZone) => resZone.campaignID === campaignID
      );
      const zoneIds = filterResZone.map((fltZone) => fltZone.zoneID);
      return zoneIds;
    }
    case ETargetTypeValue.PROVINCE: {
      const filterResProvince = restrictionProvince.filter(
        (resProv) => resProv.campaignID === campaignID
      );
      const provinceIds = filterResProvince.map(
        (fltProv) => fltProv.provinceID
      );
      return provinceIds;
    }
    case ETargetTypeValue.DISTRICT: {
      const filterResDistrict = restrictDistrict.filter(
        (resDistrict) => resDistrict.campaignID === campaignID
      );
      const districtIds = filterResDistrict.map(
        (fltDistrict) => fltDistrict.districtID
      );
      return districtIds;
    }
    case ETargetTypeValue.SEGMENT: {
      const filterResSegment = restrictionSegment.filter(
        (resSeg) => resSeg.campaignID === campaignID
      );
      const segmentIds = filterResSegment.map((fltSeg) => fltSeg.segmentID);
      return segmentIds;
    }
    case ETargetTypeValue.RETAILER: {
      const filterResShop = restrictionShop.filter(
        (resShop) =>
          resShop.campaignID === campaignID &&
          resShop.retailerType === "RETAILER"
      );
      const shopIds = filterResShop.map((fltShop) => fltShop.retailerID);
      return shopIds;
    }
  }
};

const findRetailerWithTargetType = (
  targetType: ETargetTypeValue,
  targetTypeValue: number[]
) => {
  switch (targetType) {
    case ETargetTypeValue.COUNTRY: {
      const foundAddress = address
        .filter((addr) => targetTypeValue.includes(addr.countryID))
        .map((addr) => addr.addressID);
      const findRetailer = retailer.filter((rtl) =>
        foundAddress.includes(rtl.addressID)
      );
      return findRetailer;
      break;
    }
    case ETargetTypeValue.ZONE: {
      const foundAddress = address
        .filter((addr) => targetTypeValue.includes(addr.zoneID))
        .map((addr) => addr.addressID);
      const findRetailer = retailer.filter((rtl) =>
        foundAddress.includes(rtl.addressID)
      );
      return findRetailer;
      break;
    }
    case ETargetTypeValue.PROVINCE: {
      const foundAddress = address
        .filter((addr) => targetTypeValue.includes(addr.provinceID))
        .map((addr) => addr.addressID);
      const findRetailer = retailer.filter((rtl) =>
        foundAddress.includes(rtl.addressID)
      );
      return findRetailer;
      break;
    }
    case ETargetTypeValue.DISTRICT: {
      const foundAddress = address
        .filter((addr) => targetTypeValue.includes(addr.districtID))
        .map((addr) => addr.addressID);
      const findRetailer = retailer.filter((rtl) =>
        foundAddress.includes(rtl.addressID)
      );
      return findRetailer;
      break;
    }
    case ETargetTypeValue.SEGMENT: {
      const foundAddress = address
        .filter((addr) => targetTypeValue.includes(addr.segmentID))
        .map((addr) => addr.addressID);
      const findRetailer = retailer.filter((rtl) =>
        foundAddress.includes(rtl.addressID)
      );
      return findRetailer;
      break;
    }
    case ETargetTypeValue.RETAILER: {
      const findRetailer = retailer.filter((rtl) =>
        targetTypeValue.includes(rtl.addressID)
      );
      return findRetailer;
      break;
    }
  }
};

app.post("/cal_api", (req: Request, res: Response) => {
  try {
    const campaignID = req.body.campaignID;

    const selectedCampaign = campaign.find(
      (cp) => cp.campaignID === campaignID
    );

    console.log(selectedCampaign, "selectedCampaign");

    const targetTypeValue = getTargetTypeValue(
      campaignID,
      selectedCampaign?.campaignTargetType as ETargetTypeValue
    );

    console.log(targetTypeValue, "targetTypeValue");

    const shops = findRetailerWithTargetType(
      selectedCampaign?.campaignTargetType as ETargetTypeValue,
      targetTypeValue
    );

    res.send(shops);
    // for (let i = 0; i < campaign.length; i++) {
    //   if (campaign[i]["campaignID"] == campaignID) {
    //     const retailer_zoneA = retailer.filter((retailer) => {
    //       return retailer["addressID"] == 1 || retailer["addressID"] == 3;
    //     });
    //     const retailer_zoneB = retailer.filter((retailer) => {
    //       return retailer["addressID"] == 2 || retailer["addressID"] == 4;
    //     });
    //     const customer_zoneA = dataSource.filter((customer) => {
    //       return customer["zone"] == "Zone A";
    //     });
    //     const customer_zoneB = dataSource.filter((customer) => {
    //       return customer["zone"] == "Zone B";
    //     });
    //   }
    // }
  } catch (error) {
    console.log(error);
    res.status(500).send(error);
  }
});

app.listen(port, () => {
  console.log("Server in running.");
});
